{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "newline-per-chained-call": 0,
        "eol-last": 0,
        "no-tabs": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "arrow-parens": 0,
        "no-var": "error",
        "no-console": 0
      }
}